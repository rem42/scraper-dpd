{
    "name": "rem42/scraper-dpd",
    "description": "API SOAP dpd.com",
    "require": {
        "php": "^8.1",
        "ext-soap": "*",
        "ext-mbstring": "*",
        "wsdltophp/packagebase": "~2.0"
    },
    "require-dev": {
        "rem42/php-cs-fixer-config": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Scraper\\ScraperDPD\\": "./src/"
        }
    },
    "scripts": {
        "code-style-check": "./vendor/bin/php-cs-fixer fix --dry-run --verbose",
        "code-style-fix": "./vendor/bin/php-cs-fixer fix --using-cache=no"
    }
}
